<?xml version="1.0" encoding="ASCII"?>
<model:definitions xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:di_1="http://www.omg.org/spec/DD/20100524/DI" xmlns:model="http://www.omg.org/spec/BPMN/20100524/MODEL" exporter="BonitaSoft" exporterVersion="5.9" expressionLanguage="http://groovy.codehaus.org/" targetNamespace="http://bonitasoft.com/Web_Purchase_Diagram">
  <model:collaboration id="Web_Purchase_Diagram">
    <model:documentation>Simple item purchase from Web site &#x2013; tutorial example</model:documentation>
    <model:participant id="Initiator">
      <model:documentation>Person who takes the first action to start the process</model:documentation>
    </model:participant>
    <model:participant id="Sales_Employees">
      <model:documentation>Employees authorized to handle web sales</model:documentation>
    </model:participant>
    <model:participant id="_zn3v0I0xEeK-GZ2GDNy0MQ" name="Web Purchase" processRef="Web_Purchase"/>
  </model:collaboration>
  <model:process id="Web_Purchase" name="Web Purchase">
    <model:documentation>Simple item purchase from Web site &#x2013; tutorial example</model:documentation>
    <model:userTask id="Sales_Review" name="Sales Review">
      <model:documentation>Sales employee receives order for processing</model:documentation>
    </model:userTask>
    <model:startEvent id="Start1" name="Start1">
      <model:documentation></model:documentation>
    </model:startEvent>
    <model:userTask id="Pay" name="Pay">
      <model:documentation>Customer completes credit card payment form</model:documentation>
    </model:userTask>
    <model:userTask id="More_Info" name="More Info">
      <model:documentation>Sales employee enters additional information</model:documentation>
    </model:userTask>
    <model:exclusiveGateway id="Gate1" name="Gate1">
      <model:documentation></model:documentation>
    </model:exclusiveGateway>
    <model:serviceTask id="Reject" name="Reject">
      <model:documentation>Request is rejected</model:documentation>
    </model:serviceTask>
    <model:serviceTask id="Express_Delivery" name="Express Delivery">
      <model:documentation>Send item via 3-day delivery</model:documentation>
    </model:serviceTask>
    <model:serviceTask id="Archive" name="Archive">
      <model:documentation>System archives record of transaction</model:documentation>
    </model:serviceTask>
    <model:endEvent id="End1" name="End1">
      <model:documentation></model:documentation>
      <model:terminateEventDefinition id="_zn4W4I0xEeK-GZ2GDNy0MQ"/>
    </model:endEvent>
    <model:sequenceFlow id="if_approved" name="if approved" sourceRef="Sales_Review" targetRef="Pay">
      <model:documentation></model:documentation>
      <model:conditionExpression>decision == &quot;Approve&quot;</model:conditionExpression>
    </model:sequenceFlow>
    <model:sequenceFlow id="if_not_approved" name="if not approved" sourceRef="Sales_Review" targetRef="Reject">
      <model:documentation></model:documentation>
      <model:conditionExpression>decision == &quot;Reject&quot;</model:conditionExpression>
    </model:sequenceFlow>
    <model:sequenceFlow id="add_a_comment" name="add a comment" sourceRef="Sales_Review" targetRef="More_Info">
      <model:documentation></model:documentation>
      <model:conditionExpression>decision == &quot;More Info&quot;</model:conditionExpression>
    </model:sequenceFlow>
    <model:sequenceFlow id="if_yes" name="if yes" sourceRef="Pay" targetRef="Express_Delivery">
      <model:documentation></model:documentation>
      <model:conditionExpression>chooseExpressDelivery</model:conditionExpression>
    </model:sequenceFlow>
    <model:sequenceFlow id="_16k2IBirEeCG5YN1K76MzQ" sourceRef="Express_Delivery" targetRef="Archive">
      <model:documentation></model:documentation>
    </model:sequenceFlow>
    <model:sequenceFlow id="if_no" name="if no" sourceRef="Pay" targetRef="Archive">
      <model:documentation></model:documentation>
      <model:conditionExpression>!chooseExpressDelivery</model:conditionExpression>
    </model:sequenceFlow>
    <model:sequenceFlow id="_UEI1RBitEeCG5YN1K76MzQ" sourceRef="Archive" targetRef="End1">
      <model:documentation></model:documentation>
    </model:sequenceFlow>
    <model:sequenceFlow id="_2xyqoBiuEeCG5YN1K76MzQ" sourceRef="Start1" targetRef="Gate1">
      <model:documentation></model:documentation>
    </model:sequenceFlow>
    <model:sequenceFlow id="_aiXyEBivEeCG5YN1K76MzQ" sourceRef="Gate1" targetRef="Sales_Review">
      <model:documentation></model:documentation>
    </model:sequenceFlow>
    <model:sequenceFlow id="_cKqlkBivEeCG5YN1K76MzQ" sourceRef="More_Info" targetRef="Gate1">
      <model:documentation></model:documentation>
    </model:sequenceFlow>
    <model:sequenceFlow id="_mKZmUBjJEeCG5YN1K76MzQ" sourceRef="Reject" targetRef="Archive">
      <model:documentation></model:documentation>
    </model:sequenceFlow>
    <model:dataObject id="decision" name="decision" isCollection="false">
      <model:documentation>Multiple options require a decision</model:documentation>
    </model:dataObject>
    <model:dataObject id="chooseExpressDelivery" name="chooseExpressDelivery" isCollection="false">
      <model:documentation>If yes - send express; if no - send regular post</model:documentation>
    </model:dataObject>
    <model:dataObject id="creditCardNumber" name="creditCardNumber" isCollection="false">
      <model:documentation>Customer's Credit Card</model:documentation>
    </model:dataObject>
    <model:dataObject id="expirationDate" name="expirationDate" isCollection="false">
      <model:documentation>Card valid until</model:documentation>
    </model:dataObject>
    <model:dataObject id="comment" name="comment" isCollection="false">
      <model:documentation>Additional information supplied by sales employee</model:documentation>
    </model:dataObject>
    <model:dataObject id="customerEmail" name="customerEmail" isCollection="false">
      <model:documentation></model:documentation>
    </model:dataObject>
    <model:dataObject id="customerName" name="customerName" isCollection="false">
      <model:documentation>Customer&#x2019;s full name</model:documentation>
    </model:dataObject>
    <model:dataObject id="customerPhone" name="customerPhone" isCollection="false">
      <model:documentation></model:documentation>
    </model:dataObject>
    <model:dataObject id="products" name="products" isCollection="false">
      <model:documentation>Available products</model:documentation>
    </model:dataObject>
  </model:process>
